# Database Configuration (Required)
# For Supabase: Get these from your Supabase project settings
DATABASE_URL="postgresql://postgres:[password]@[host]:6543/postgres?pgbouncer=true&connection_limit=1"
DIRECT_URL="postgresql://postgres:[password]@[host]:5432/postgres"

# AI Service Configuration (Required)
OPENROUTER_API_KEY="your_openrouter_api_key_here"
OPENROUTER_MODEL="anthropic/claude-3.5-sonnet"

# Whop Integration (Required)
WHOP_APP_API_KEY="your_WHOP_APP_API_KEY_here"
WHOP_AGENT_USER_ID="your_bot_user_id_here"

# Application Configuration (Optional)
NODE_ENV="development"
PORT="3000"

# Rate Limiting (Optional - defaults provided)
# AI_RATE_LIMIT_REQUESTS_PER_MINUTE="10"
# MESSAGE_RATE_LIMIT_REQUESTS_PER_MINUTE="30"

# Logging (Optional)
# LOG_LEVEL="info"
# ENABLE_DATABASE_LOGGING="true"

# Security (Optional)
# CORS_ORIGINS="https://yourdomain.com"

# Health Check (Optional)
# HEALTH_CHECK_SECRET="your_secret_for_health_checks" 